citHeader("To cite geodimension in publications use:")

citEntry(entry = "Manual",
  title        = "{geodimension}: Definition of Geographic Dimensions",
  author       = personList(as.person("Jose Samos")),
  year         = "2020",
  note         = "R package version 1.0.0",
  url          = "https://CRAN.R-project.org/package=geodimension",

  textVersion  =
  paste("Jose Samos (2020).",
        "geodimension: Definition of Geographic Dimensions.",
        "https://CRAN.R-project.org/package=geodimension.")
)
